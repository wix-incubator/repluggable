{
  "name": "@wix/repluggable",
  "version": "0.5.20",
  "description": "Allows composition of a React-with-Redux application entirely from a list of pluggable packages",
  "author": {
    "name": "Responsive FEDs",
    "email": "responsive-feds@wix.com"
  },
  "license": "ISC",
  "unpkg": true,
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "scripts": {
    "start": "yoshi start",
    "build": "yoshi build",
    "test": "yoshi test",
    "posttest": "npm run lint",
    "lint": "yoshi lint --fix",
    "release": "yoshi release"
  },
  "main": "dist/statics/app.bundle.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "react": "~16.6.0",
    "react-dom": "~16.6.0",
    "react-redux": "^5.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.16",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.122",
    "@types/react": "^16.7.18",
    "@types/react-redux": "^7.0.0",
    "@types/requirejs": "^2.1.31",
    "@wix/tslint-config-responsive-editor": "^1.0.27",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "husky": "^2.1.0",
    "jest-yoshi-preset": "^4.3.2",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "puppeteer": "^1.15.0",
    "typescript": "^3.4.5",
    "yoshi": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "yoshi lint --fix true",
        "git add"
      ]
    },
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "jest": {
    "preset": "jest-yoshi-preset"
  }
}
