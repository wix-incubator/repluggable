    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/enzyme": "^3.1.16",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.122",
    "@types/react": "^16.7.18",
    "@types/react-redux": "^7.0.0",
    "@types/requirejs": "^2.1.31",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
