    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-transform-stub": "^2.0.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "typescript": "^3.4.5",
